CXX_STD = CXX11
RM = rm

C_OBJS = oscarc.o
FT_OBJS = dbdc.o plqdf1.o

all:
	@$(MAKE) $(SHLIB)
	@rm -f *.mod *.o

clean:	
	$(RM) *.mod *.o  
	echo Clean done

USE_FC_TO_LINK =
PKG_FFLAGS = $(SHLIB_OPENMP_FFLAGS)
PKG_LIBS = $(SHLIB_OPENMP_FFLAGS) $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS)
$(SHLIB): $(C_OBJS) $(FT_OBJS)

oscarc.c: dbdc.o

